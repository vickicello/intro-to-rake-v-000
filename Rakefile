namespace :greeting do
  desc 'outputs hello to the terminal'
   task :hello do
     puts "hello from Rake!"
   end

   desc 'outputs hola to the terminal'
   task :hola do
     puts "hola de Rake!"
   end

  desc 'outputs bonjour to the terminal'
  task :bonjour do
    puts "bonjour de Rake"
  end
 
end
